{
  "name": "joel.fm",
  "private": true,
  "description": "joél hawkins torres portfolio website, designed with gatsby.js",
  "version": "1.1.5",
  "author": "joél <joel@joel.fm> (https://joel.fm)",
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@react-icons/all-files": "^4.1.0",
    "gatsby": "^3.7.2",
    "gatsby-image": "^3.7.1",
    "gatsby-plugin-emotion": "^6.7.1",
    "gatsby-plugin-feed": "^3.7.1",
    "gatsby-plugin-google-analytics": "^3.7.1",
    "gatsby-plugin-image": "^1.7.1",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-purgecss": "^6.0.2",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-copy-linked-files": "^4.4.1",
    "gatsby-remark-images": "^5.4.1",
    "gatsby-remark-prismjs": "^5.4.1",
    "gatsby-remark-responsive-iframe": "^4.4.1",
    "gatsby-remark-smartypants": "^4.4.1",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-transformer-remark": "^4.4.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "normalize.css": "^8.0.1",
    "polished": "^4.1.3",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@types/babylon": "^6.16.6",
    "autoprefixer": "^10.2.6",
    "babel-preset-gatsby": "^1.7.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "esm": "^3.2.25",
    "husky": "^5.2.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.4.1",
    "prettier-eslint": "^12.0.0",
    "pretty-quick": "^3.1.1",
    "yarn-run-all": "^3.1.1"
  },
  "homepage": "https://joel.fm/",
  "keywords": [
    "react",
    "gatsby",
    "pay me plz",
    "hwknsj",
    "joel hawkins",
    "joel torres",
    "jtor94",
    "nike",
    "web design",
    "emotion",
    "minimal-ui",
    "portfolio",
    "@joel.biz",
    "joel.fm"
  ],
  "license": "MIT",
  "main": "gatsby-config.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hwknsj/joel.fm"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS='-r esm' gatsby build",
    "develop": "cross-env NODE_OPTIONS='-r esm' gatsby develop",
    "format": "run-s format:sort-imports format:prettier",
    "format:prettier": "prettier --config .prettierrc.js --loglevel warn --write 'src/**/*.{js,jsx,ts,tsx,json}'",
    "check:prettier": "prettier --config .prettierrc.js --loglevel warn --check 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format:sort-imports": "import-sort --write 'src/**/*.{js,jsx,ts,tsx}'",
    "check:sort-imports": "import-sort 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --config .eslintrc.js --color 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --config .eslintrc.js --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "clean": "cross-env NODE_OPTIONS='-r esm' gatsby clean",
    "start": "run-s develop",
    "dev": "run-s develop",
    "test": "echo 'Write tests! -> https://gatsby.dev/unit-testing'",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --config .eslintrc.js --max-warnings 0",
      "import-sort --write",
      "prettier --config .prettierrc.js --write",
      "git add"
    ],
    "src/**/*.{scss,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1"
  }
}
