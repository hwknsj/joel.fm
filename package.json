  "homepage": "https://joel.fm/",
  "keywords": [
    "react",
    "gatsby",
    "pay me plz",
    "portfolio",
    "@joel.biz"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/hwknsj/joel.fm"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"-r esm\" gatsby build",
    "develop": "cross-env NODE_OPTIONS=\"-r esm\" gatsby develop",
    "format": "run-s format:sort-imports format:prettier",
    "format:prettier": "prettier --loglevel warn --single-quote --write './**/*.{js,jsx,ts,tsx,json,scss,css}'",
    "format:sort-imports": "import-sort --write './**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --max-warnings 0 'src/**/*.{js,jsx,ts,tsx}'",
    "clean": "gatsby clean",
    "start": "yarn run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "postinstall": "husky install"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "trailingComma": "none",
    "jsxSingleQuote": true,
    "useTabs": false,
    "tabWidth": 2
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings 0",
      "import-sort --write",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{scss,css}": [
      "prettier --write",
      "git add"
    ],
    "*.js": "eslint --cache --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
