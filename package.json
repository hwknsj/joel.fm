    "gatsby-plugin-offline": "^5.5.2",
    "gatsby-plugin-purgecss": "^6.1.0",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-sharp": "^4.5.2",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-copy-linked-files": "^5.5.0",
    "gatsby-remark-images": "^6.5.2",
    "gatsby-remark-prismjs": "^6.5.0",
    "gatsby-remark-responsive-iframe": "^5.5.0",
    "gatsby-remark-smartypants": "^5.5.0",
    "gatsby-source-filesystem": "^4.5.2",
    "gatsby-transformer-remark": "^5.5.2",
    "gatsby-transformer-sharp": "^4.5.0",
    "normalize.css": "^8.0.1",
    "polished": "^4.1.3",
    "prismjs": "^1.26.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sharp": "^0.29.3",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/eslint-plugin": "^11.7.0",
    "@types/babylon": "^6.16.6",
    "autoprefixer": "^10.4.2",
    "babel-preset-gatsby": "^2.5.2",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^7.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "esm": "^3.2.25",
    "husky": "^7.0.4",
    "import-sort-cli": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "pretty-quick": "^3.1.3",
    "yarn-run-all": "^3.1.1"
  },
  "homepage": "https://joel.fm/",
  "keywords": [
    "react",
    "gatsby",
    "pay me plz",
    "hwknsj",
    "joel hawkins",
    "joel torres",
    "jtor94",
    "nike",
    "web design",
    "emotion",
    "minimal-ui",
    "portfolio",
    "@joel.biz",
    "joel.fm"
  ],
  "license": "MIT",
  "main": "gatsby-config.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hwknsj/joel.fm"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "run-s import-sort:format prettier:format",
    "prettier:format": "prettier --config .prettierrc.js --loglevel warn --write 'src/**/*.{js,jsx,ts,tsx,json,html,css,scss}'",
    "prettier:check": "prettier --config .prettierrc.js --loglevel warn --check 'src/**/*.{js,jsx,ts,tsx,json,html,css,scss}'",
    "import-sort:format": "import-sort --write 'src/**/*.{js,jsx,ts,tsx}'",
    "import-sort:check": "import-sort 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint --config .eslintrc.js --color 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --config .eslintrc.js --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "clean": "gatsby clean",
    "start": "run-s develop",
    "dev": "run-s develop",
    "test": "echo 'Write tests! -> https://gatsby.dev/unit-testing'",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module"
    }
  },
  "lint-staged": {
    "src/**/*.{j,t}s(x)?": [
      "eslint --config .eslintrc.js",
      "import-sort --write",
      "prettier --config .prettierrc.js --write"
    ],
    "src/**/*.{scss,css}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1"
  }
}
