{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noEmit": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/assets/*": [
        "content/assets/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/images/*": [
        "src/images/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/projects/*": [
        "content/projects/*"
      ],
      "@/styles/*": [
        "src/components/styles/*"
      ],
      "@/templates/*": [
        "src/templates/*"
      ],
      "assets/*": [
        "content/assets/*"
      ]
    },
    "resolveJsonModule": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./gatsby-node.ts",
    "./gatsby-config.ts",
    "./gatsby-browser.tsx",
    "./gatsby-ssr.tsx",
    "./src/gatsby-types.d.ts",
    "./emotion.d.ts",
    "./plugins/**/*"
  ]
}